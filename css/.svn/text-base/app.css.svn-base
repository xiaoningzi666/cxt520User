.axislist{
	background: #fff;
	padding: 15px 10px;
}
.axislist ul li{
	position: relative;
	width:100%;
	height: 50px;
}
.axislist ul li .axisItem{
	display: table;
	width: 45%;
	height: 60px;
	bottom: 0;
	
}
.axislist ul{
	margin-top: 45px;
}

.axislist ul li .axisItem .axisp{
	display: table-cell;
	vertical-align: middle;
	text-align: center;
}

.axislist ul li:nth-child(even) .axisItem{
	position: absolute;
	left: 0;
}
.axislist ul li:nth-child(even) .axisItemon{
	background: url(../images/axisOnLBg.png) no-repeat center center;
	background-size: contain;
}
.axislist ul li:nth-child(even) .axisItemoff{
	background: url(../images/axisLbg.png) no-repeat center center;
	background-size: 100% 90%;
  margin-left: 1.4rem;
}
.axislist ul li:nth-child(odd) .axisItemoff{
	background: url(../images/axisRbg.png) no-repeat center center;
	background-size: 100% 90%;
  margin-right: 1.4rem;
}
.axislist ul li:nth-child(odd) .axisItemon{
	background: url(../images/axisOnRBg.png) no-repeat center center;
	background-size: contain;
}
.axislist ul li .axisItemoff{
	width: 40%;
	height: 40px;

}
.axislist ul li:nth-child(odd) .axisItem{
	position: absolute;
	right: 0;
}
.axislist ul li .axisLoff{
	position: absolute;
	left: 50%;
	content: "";
	width: 1px;
	height: 45px;
	background: #ccc;
}
.axislist ul li .axisLon{
	position: absolute;
	left: 50%;
	content: "";
	width: 1px;
	height: 50px;
	background: #00a0e9;
	bottom: 0px;
}
.axislist ul li .axisLon:before,
.axislist ul li .axisLoff:before,
.axislist ul li:last-child:after{
	position: absolute;
	left: 50%;
	content:'';
	width: 6px;
	height: 6px;
	border-radius: 50%;
	-webkit-border-radius: 50%;
}
.axislist ul li .axisLon:before{
	background: #00a0e9;
	top: 15px;
	margin-left: -0.3rem;
}
.axislist ul li .axisLoff:before{
	background: #ccc;
	top: 18px;
	margin-left: -0.3rem;
}
.axislist ul li:first-child:before{
	position: absolute;
    content: '';
    left: 50%;
    width: 30px;
    height: 70px;
    margin-left: -15px;
    top: -52px;
    background: url(../images/axisendff.png) no-repeat center center;
    background-size: 80%;
    background-color: #FFFFFF;
    z-index: 2;
}
.axislist ul li:last-child:after{
	bottom: 0;
	border:1px solid #00a0e9;
	margin-left: -3px;
	bottom: -5px;
}
.axisItemon .axisp1 .score,.axisItemon .axisp2 .score{
	font-size: 1.5rem;
	color: #f3a238;
	
}
.axisp2 span:last-child{
	color: #888;
	font-size: 0.8rem;
}
