<script>
$(function(){
	var oilL=(windowW-$('.oilcon').width())/2;
	$('.oilwrap').css({
		"height":windowH
	})
	$('.noticTipTxt').click(function(){
		$('.oilwrap').show();
	})
	$('.oilwrap').click(function(){
		$('.oilwrap').hide();
	})
	$.ajax({
		type:"get",
		url:apiUrl+"/appcms/getcontents",
		data:{
			"region":storage['region'],
			"columnCode":"H01M02",
			"isGetImage":1,
			"pageNum":1,
			"pageSize":0
			
		},
		dataType:"json",
		success:function(data){
			var html='';
			if(data.return_code=="SUCCESS"){
				html+='<div class="hdp-box">'+
						'<div id="slider" class="swipe">'+
							'<ul class="piclist">';
							$.each(data.pageInfo.list, function(index,item) {
									html+='<li><a href="'+item.url+'"><img src="'+item.imageList[0].url+'"></a></li>';
								});
					html+='</ul>'+
						'</div>'+
						'<div class="count">'+
							'<b class="bi">1</b>/<b class="bcount"></b>'+
						'</div>'+
					'</div>'
				$('#banner_wrap').html(html)
				var num=$('#slider').find('li').size();
					$('.bcount').text(num);	
					$('.b_btn').click(function(){
					$(this).toggleClass('b_btn_active');
					$('.intro').toggle();
				})
				var tt=new TouchSlider({
					'id':'slider',
					'auto':'0',
					'fx':'ease-out',
					'direction':'left',
					'speed':600,
					'timeout':5000,
					'before':function(index){
						var es=document.getElementById('slider').getElementsByTagName('li');
						var it=$(es[index]).index()+1;	
						$('.bi').text(it);	
						var tx=$(es[index]).find('p').text();	
						$('.title').text(tx);
					}
				});
			}
		}
	});
})


